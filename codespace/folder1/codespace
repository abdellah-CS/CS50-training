           //including libraries
#include <stdio.h>
#include <cs50.h>
           //functions
void rec(int n);
int main(void)
{
    rec(0);
    printf("\n");
}
void rec(int n)
{
    for(int i = 0; i < n; i += 1)
    {
    printf("#");
    }
    if(n == 4)
    {
        return;
    }
    printf("\n");
    rec(n+1);
}